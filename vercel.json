{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.js"
    }
  ],
  "env": {
    "mailID": "@mailID",
    "password": "@password",
    "mailPort": "@mailPort"
  },
  "functions": {
    "server/index.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}

